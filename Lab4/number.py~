class Integer (object):
	def __init__(self, x, y):
		self.number = x
		self.positive = y
		
	def display(self):
		print self.positive + str(self.number)
		
class NegativeInteger (Integer):
	def __init__(self, x):
		 super (NegativeInteger, self).__init__(x, "-")

if __name__ == "__main__":
	x = Integer (5, "+")
	x.display()
	z = NegativeInteger(234)
	z.display()
	


	
